{
    "workbench.editorAssociations": {
        "*.sav": "default"
    },
    "editor.autoClosingQuotes": "always",
    "editor.autoSurround": "brackets",
    "editor.autoClosingBrackets": "always",
    "editor.unicodeHighlight.nonBasicASCII": false,
    "security.workspace.trust.untrustedFiles": "open",
    "files.maxMemoryForLargeFilesMB": 9096,
    "editor.inlineSuggest.enabled": true,
    "github.copilot.enable": {
        "length": 2000,
        "*": true,
        "yaml": false,
        "plaintext": false,
        "markdown": false
    },
    "vscode-neovim.useWSL": false,
    "vscode-neovim.neovimExecutablePaths.win32": "C:\\nvim-win64\\nvim-win64\\bin\\nvim.exe",
    "vscode-neovim.neovimInitVimPaths.win32": "C:\\Users\\Administrator\\AppData\\Local\\nvim\\init.vim",
    "vscode-neovim.highlightGroups.unknownHighlight": "vim",
    "editor.scrollBeyondLastLine": false,
    "workbench.colorCustomizations": {
        "peekViewEditor.background": "#01041a",
        "peekViewResult.background": "#054c8a",
        "peekView.border": "#04fafa"
    },
    "vim.leader": "<Space>",
    "vim.showcmd": true,
    "vim.showMarksInGutter": true,
    "vim.inccommand": "append",
    "vim.incsearch": true,
    "vim.hlsearch": true,
    "vim.sneak": true,
    "vim.easymotion": true,
    "vim.startofline": false,
    "vim.insertModeKeyBindingsNonRecursive": [
        {
            "before": [
                "j",
                "k"
            ],
            "after": [
                "<Esc>"
            ]
        }
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<leader>",
                "y"
            ],
            "after": [],
